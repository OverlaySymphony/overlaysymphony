    "react": "^19.0.0"
  },
  "devDependencies": {
    "@overlaysymphony/tooling": "workspace:*",
    "@types/react": "^19.1.10",
    "@vitest/coverage-v8": "catalog:",
    "depcheck": "catalog:",
