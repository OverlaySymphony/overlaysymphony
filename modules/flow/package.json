{
  "name": "@overlaysymphony/flow",
  "version": "0.2.3",
  "description": "Flow logic module for the OverlaySymphony interactive streaming framework.",
  "homepage": "https://github.com/OverlaySymphony/overlaysymphony",
  "repository": {
    "type": "git",
    "url": "https://github.com/OverlaySymphony/overlaysymphony"
  },
  "type": "module",
  "exports": {
    ".": "./src/main.ts",
    "./modules": "./src/modules/index.ts",
    "./editor": "./src/Editor/index.ts",
    "./viewer": "./src/Viewer/index.ts",
    "./package.json": "./package.json"
  },
  "imports": {
    "#shared/*": "./src/shared/*/index.ts"
  },
  "scripts": {
